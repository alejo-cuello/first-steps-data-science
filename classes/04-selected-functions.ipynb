   "source": [
    "N = 100\n",
    "\n",
    "def H(x):\n",
    "    Y = np.zeros(len(x))\n",
    "    for idx,x in enumerate(x):\n",
    "        if x>=0:\n",
    "            Y[idx]=1\n",
    "    return Y\n",
    "\n",
    "x = np.linspace(-10,10,num=N)\n",
    "y=H(x)\n",
    "\n",
    "plt.plot(x,y)\n",
    "plt.grid()"
   ]
  }
